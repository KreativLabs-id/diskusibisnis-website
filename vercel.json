{
  "name": "diskusi-bisnis",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "frontend/app/**": {
      "maxDuration": 30
    }
  },
  "regions": ["sin1"],
  "framework": "nextjs"
}
